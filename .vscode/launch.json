    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "flaskApp.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                // "UPLOAD_FOLDER": "/Users/eliaweiss/tmpUpload/",
                // "UPLOAD_FOLDER": "/home/ec2-user/upload/",
                "IS_LOCAL": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host=0.0.0.0",
                "--port=5001"
            ],
            "jinja": true
        },
        {
            "name": "svm",
            "type": "debugpy",
            "request": "launch",
            "program": "svmMain.py",
            "console": "integratedTerminal",
        },   
        {
            "name": "Inference_withTblDet",
            "type": "debugpy",
            "request": "launch",
            "program": "MobileSAMv2/Inference_withTblDet.py",
            "console": "integratedTerminal",
        },        
        {
            "name": "Inference boxes.py",
            "type": "debugpy",
            "request": "launch",
            "program": "MobileSAMv2/Inference_withBoxes.py",
            "console": "integratedTerminal",
        },        
        // {
        //     // note: require yolo for box predication
        //     "name": "MobileSAMv2/Inference.py",
        //     "type": "debugpy",
        //     "request": "launch",
        //     "program": "MobileSAMv2/Inference_withYolo.py",
        //     "console": "integratedTerminal",
        // },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}